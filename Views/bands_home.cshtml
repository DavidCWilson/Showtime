@{
  Layout="Shared/_Layout.cshtml";
}
<p>
  Bands:
</p>
@if (Model.Count != 0)
{
  <ul>
  @foreach (var band in @Model)
  {
    <li><a href="/bands/@band.GetId()">@band.GetName()</a></li>
  }
  </ul>
}
<p><a href="/bands/add">Add</a></p>
<div id="navigation">
  <p><a href="/venues">Venues</a></p>
  <p><a href="/bands">Bands</a></p>
  <p><a href="/genres">Genres</a></p>
  <p><a href="/">Home</a></p>
</div>
